alter_group_vim() {
	associations=(usr/bin/vim:usr/libexec/vim/vim)
	priority=50
}

alter_group_ex() {
	associations=(usr/bin/ex:usr/libexec/vim/ex)
	priority=50
}

alter_group_view() {
	associations=(usr/bin/view:usr/libexec/vim/view)
	priority=50
}

alter_group_vimdiff() {
	associations=(usr/bin/vimdiff:usr/libexec/vim/vimdiff)
	priority=50
}

alter_group_vimtutor() {
	associations=(usr/bin/vimtutor:usr/libexec/vim/vimtutor)
	priority=50
}

alter_group_editor() {
	associations=(usr/bin/editor:usr/bin/vim
		usr/share/man/man1/editor.1.gz:usr/share/man/man1/vim.1.gz)
	priority=50
}

alter_group_vi() {
	associations=(usr/bin/vi:usr/bin/vim
		usr/share/man/man1/vi.1.gz:usr/share/man/man1/vim.1.gz)
	priority=30
}
